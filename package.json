{
  "name": "traffic-traversal",
  "version": "1.4.1",
  "description": "Calculate the weights between each vertex node and help you find the fastest route.",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "typings": "./dist/typings/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/typings/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    },
    "./Helpers/VertexEncoder": {
      "types": "./dist/typings/Helpers/VertexEncoder.d.ts",
      "require": "./dist/cjs/Helpers/VertexEncoder.cjs",
      "import": "./dist/esm/Helpers/VertexEncoder.mjs"
    }
  },
  "scripts": {
    "test": "jest -t",
    "build": "node build/index.js && tsc"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "traversal",
    "traffic",
    "vertex",
    "route",
    "SPF",
    "SPFA",
    "dijkstra",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/izure1/traffic-traversal.git"
  },
  "author": "izure1 <admin@izure.org>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "esbuild": "^0.25.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
